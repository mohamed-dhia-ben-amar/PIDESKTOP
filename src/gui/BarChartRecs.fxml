<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="691.0" prefWidth="852.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.BarChartRecsController">
   <children>
      <BarChart fx:id="RecBarChart" layoutX="40.0" layoutY="25.0" prefHeight="606.0" prefWidth="778.0" title="States Chart">
        <xAxis>
          <CategoryAxis fx:id="stateAxis" label="State" side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis fx:id="countAxis" label="Number" side="LEFT" />
        </yAxis>
      </BarChart>
      <Button fx:id="loadChart" layoutX="389.0" layoutY="636.0" mnemonicParsing="false" onMouseClicked="#loadchart" text="Load Chart" />
   </children>
</AnchorPane>
