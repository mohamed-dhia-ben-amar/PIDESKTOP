<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="aces.gui.AjouterArticleController">
   <children>
      <Pane fx:id="idajouter" layoutX="-1.0" prefHeight="400.0" prefWidth="155.0" style="-fx-background-color: darkblue;">
         <children>
            <Button fx:id="back" layoutX="45.0" layoutY="82.0" mnemonicParsing="false" onAction="#returnback" text="Back" />
         </children>
      </Pane>
      <TextField fx:id="titre" layoutX="196.0" layoutY="61.0" prefHeight="31.0" prefWidth="209.0" promptText="Titre" />
      <TextField fx:id="contenu" accessibleRole="TEXT_AREA" alignment="TOP_LEFT" layoutX="197.0" layoutY="101.0" prefHeight="105.0" prefWidth="209.0" promptText="Contenu" />
      <Button fx:id="ajouter" layoutX="447.0" layoutY="305.0" mnemonicParsing="false" onAction="#ajouterarticle" text="Ajouter" />
      <TextField fx:id="image" layoutX="201.0" layoutY="216.0" prefHeight="31.0" prefWidth="199.0" promptText="image" />
      <TextField fx:id="date" disable="true" layoutX="202.0" layoutY="262.0" prefHeight="31.0" prefWidth="199.0" promptText="date de creation" />
      <Button fx:id="choice" layoutX="449.0" layoutY="216.0" mnemonicParsing="false" onAction="#choiceimage" text="choisir" />
   </children>
</AnchorPane>
